
        <%- include("header") -%>
        <div class="box" id="heading">
            <h1><%= KindOfDay %></h1>
        </div>
        <div class="box" id="body-work">
            <div class="item">
                <%for(var i=0; i<newListItem.length; i++){%>
                    <input type="checkbox">
                    <p><%= newListItem[i] %></p>
                    <br>
               <% }%>
            </div>
            <form method="post" class="item">
                <label for="newinp">Input</label>
                <input type="text" name ="inp" placeholder="New item" autocomplete="off">
                <button type="submit" name = "button" action="/">+</button>
            </form>
        </div>
        <%- include("footer") -%>
